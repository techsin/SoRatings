{
  "name": "SO Google Enhancer",
  "description": "This extension shows SO questions ratings on side and if it has best answer selected.",
  "version": "1.0.0",
  "icons": { "128": "icon.png" },
  "author": "Muhammad Umer",
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "content_scripts": [
      {
        "matches": ["https://www.google.com/search*","https://www.google.com/webhp*"],
        "css": ["styles.css"],
        "js": ["jquery.min.js", "myscript.js"]
      }
    ],
  "manifest_version": 2
}
